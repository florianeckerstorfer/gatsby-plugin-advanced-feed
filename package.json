{
  "name": "@fec/gatsby-plugin-advanced-feed",
  "version": "1.2.0",
  "description": "Gatsby plugin that generates RSS 2, Atom and JSON feeds for your site.",
  "author": "Florian Eckerstorfer <florian@eckerstorfer.net>",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "test": "jest",
    "test:integration": "./__tests__/integration.spec.sh",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "coverage": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "./setupTests.js"
    ]
  },
  "private": false,
  "license": "MIT",
  "homepage": "https://florian.ec/projects/gatsby-plugin-advanced-feed",
  "keywords": [
    "atom",
    "feed",
    "gatsby",
    "gatsby-plugin",
    "rss",
    "json feed"
  ],
  "bugs": {
    "url": "https://github.com/fec/gatsby-plugin-advanced-feed/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/florianeckerstorfer/gatsby-plugin-advanced-feed.git"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "peerDependencies": {
    "gatsby": "^2.4.0"
  },
  "dependencies": {
    "@babel/runtime": "7.12.13",
    "dayjs": "^1.8.16",
    "feed": "^4.0.0",
    "gatsby": "^2.31.1",
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "7.12.10",
    "babel-eslint": "^10.0.2",
    "babel-preset-gatsby-package": "0.11.0",
    "cross-env": "^7.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^26.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.9.0"
  }
}
